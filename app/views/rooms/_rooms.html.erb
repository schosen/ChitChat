<div>
  <%= link_to 'Create a room', new_room_path %>

</div>

<% if @rooms.present? %>
  <nav class="nav">
    <% @rooms.each do |room| %>
      <%= link_to room.name, room_path(room), class: 'nav-link' %>

    <% end %>
  </nav>
<% else %>
  <div class="">
    There are no rooms
  </div>
<% end %>

      <%= link_to 'Delete', room_path(room), method: :delete, data: { confirm: 'Are you sure?' } %>